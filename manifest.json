{
  "manifest_version": 3,
  "name": "Tab Limiter",
  "description": "Auto-close the oldest tab when you exceed 10 open tabs.",
  "version": "1.0",
  "permissions": ["tabs"],
  "background": { "service_worker": "background.js" },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
